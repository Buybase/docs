{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Buybase",
  "logo": {
    "dark": "/logo/buybase-light.svg",
    "light": "/logo/buybase-dark.svg"
  },
  "favicon": "/favicon.svg",
  "rounded": "default",
  "font": {
    "headings": {
      "family": "Geist"
    },
    "body": {
      "family": "Geist"
    }
  },
  "colors": {
    "primary": "#323232",
    "light": "#323232",
    "dark": "#323232",
    "background": {
      "dark": "#323232",
      "light": "#323232"
    },
    "anchors": {
      "from": "#323232",
      "to": "#323232"
    }
  },
  "api": {
    "baseUrl": "https://api.buybase.ai"
  },
  "topbarLinks": [
    {
      "name": "Community",
      "url": "https://discord.gg/9EXhpj7nDZ"
    }
  ],
  "topbarCtaButton": {
    "name": "Dashboard",
    "url": "https://dashboard.buybase.ai"
  },
  "tabs": [
    {
      "name": "Docs",
      "url": "docs"
    },
    {
      "name": "API",
      "url": "api-reference"
    },
    {
      "name": "Changelog",
      "url": "changelog"
    }
  ],
  "navigation": [
    {
      "group": "General",
      "pages": ["/docs/welcome", "/docs/quickstart"]
    },
    {
      "group": "Reference",
      "pages": [
        "/docs/use-cases",
        "/docs/supported-platforms",
        "/docs/security",
        "/docs/response-codes"
      ]
    },
    {
      "group": "Misc",
      "pages": ["/docs/support", "/docs/faqs"]
    },
    {
      "group": "Search",
      "pages": ["/api-reference/search/get"]
    },
    {
      "group": "Buy",
      "pages": ["/api-reference/buy/post"]
    },
    {
      "group": "Users",
      "pages": ["/api-reference/users/post", "/api-reference/users/get", "/api-reference/users/delete"]
    },
    {
      "group": "Stores",
      "pages": ["/api-reference/stores/get"]
    },
    {
      "group": "Apps",
      "pages": ["/api-reference/apps/get"]
    },
    {
      "group": "Changelog",
      "pages": ["/changelog/overview"]
    }
  ],
  "footerSocials": {
    "x": "https://x.com/buybaseai",
    "linkedin": "https://linkedin.com/company/buybaseai"
  }
}